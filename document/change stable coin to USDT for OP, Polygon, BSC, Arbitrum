        ftm-usdt 
        usdc-usdt 
        boo-ftm-usdt
        btc-ftm-usdt
        eth-ftm-usdt
        miMatic-usdc-usdt
        tomb-ftm-usdt
        tor-ftm-usdt

        0xf7a2bb5C3592edE90cf33f9832529404D650CF8c

